    "react-dom": "^15.5.3",
    "react-hot-loader": "next",
    "react-redux": "^5.0.4",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(scss|css|sass|stylus|less)$": "identity-obj-proxy",
