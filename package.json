    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(scss|css|sass|stylus|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)(?:[?]?#[a-zA-Z]+)?$": "identity-obj-proxy"
    }
  }
